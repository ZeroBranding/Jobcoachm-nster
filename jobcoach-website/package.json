{
  "name": "jobcoach-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:server\"",
    "dev:next": "next dev",
    "dev:server": "nodemon --watch server --exec ts-node server/index.ts",
    "build": "next build && tsc -p server/tsconfig.json",
    "start": "NODE_ENV=production node dist/server/index.js",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@react-three/drei": "^9.92.7",
    "@react-three/fiber": "^8.15.12",
    "@types/three": "^0.160.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "framer-motion": "^10.16.16",
    "gsap": "^3.12.4",
    "jsonwebtoken": "^9.0.2",
    "leva": "^0.9.35",
    "next": "14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-signature-canvas": "^1.0.6",
    "react-toastify": "^9.1.3",
    "react-tsparticles": "^2.12.2",
    "three": "^0.160.0",
    "tsparticles": "^3.0.2",
    "tsparticles-slim": "^2.12.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/react-signature-canvas": "^1.0.5",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.32",
    "prisma": "^5.7.1",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
